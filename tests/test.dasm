    OUT text_writing

    SET A, 3
    SET X, 0
    SET Y, data
    HWI 0

    OUT text_reading

    SET A, 2
    SET X, 0
    SET Y, diskbuffer
    HWI 0

    OUT text_result
    OUT diskbuffer
